{
    "expo": {
      "name": "Hyperpass",
      "slug": "Hyperpass",
      "version": "1.0.0",
      "orientation": "portrait",
      "entryPoint": "./app/App.js",
      "icon": "./app/assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./app/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.anonymous.Hyperpass"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./app/assets/adaptiveicon.png",
          "backgroundColor": "#ffffff"
        },
        "package": "com.anonymous.Hyperpass"
      },
      "web": {
        "favicon": "./app/assets/favicon.png"
      }
    }
  }